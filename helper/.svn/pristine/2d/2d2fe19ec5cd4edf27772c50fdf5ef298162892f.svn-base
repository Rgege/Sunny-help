html, body
{
	padding: 0;
	margin: 0;
}
body
{
	background-color: #373e4a;
	font-family: Arial, Helvetica, Verdana;
	font-size: 14px;
	line-height: 22px;
	color: #515b6d;
	position: relative;
	-webkit-text-size-adjust: none;
	overflow:hidden;
}
body *
{
	text-shadow: none;
}
h1, h2, h3, h4, h5, h6
{
	line-height: 1;
	font-weight: bold;
	margin: 20px 0 10px 0;
}
h1, h2, h3
{
	font-size: 18px;
}
h4, h5, h6
{
	font-size: 16px;
}
p
{
	margin: 0 0 10px 0;
}
a, a:link, a:active, a:visited, a:hover
{
	color: inherit;
	text-decoration: underline;
}

#menu:not(.mm-menu)
{
	display: none;
}
#menu{
	border: 1px solid rgba(34, 25, 25, 0.2);
	border-top: none;
	box-shadow: 0px -1px 8px rgba(34, 25, 25, 0.2) inset;
}
.navbar{
	/*background-color: #373E4A;*/
	background-color: #1c1f29;
	border: 1px solid rgba(34, 25, 25, 0.2);
	border-top: none;
	border-left: none;
	border-right: none;
	width: 100%;
	height: 40px;
	margin-bottom: 20px;
	/*box-shadow:0px 0px 6px white;*/
	box-shadow: 0px 1px 3px rgba(34, 25, 25, 0.2);
}
.navbar .container{
	width: 100%;
	height: 10%;
}
.navbar-header .glyphicon
{
	width: 20%;
	height: 30%;
	padding: 11px;
	position: absolute;
	top: 0;
	left: 0;
}
